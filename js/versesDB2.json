verses = [{"0":{"spanish":{"verse":"Yo soy el Señor tu Dios y nunca cambio.","book":"Malaquías 3:6"},"english":{"verse":"","book":""}},"1":{"spanish":{"verse":"Estoy lleno de misericordia y gracia y reboso de amor.","book":"Salmos 103:8"},"english":{"verse":"","book":""}},"2":{"spanish":{"verse":"Las intenciones de Mi corazón se mantendrán firmes para siempre.","book":"Salmos 33:11"},"english":{"verse":"","book":""}},"3":{"spanish":{"verse":"Te he creado a Mi imagen y con Mi bendición especial.","book":"Génesis 1:27-28"},"english":{"verse":"","book":""}},"4":{"spanish":{"verse":"He decidido adoptarte en Mi familia antes de la creación del Mundo.","book":"Efesios 1:4-5"},"english":{"verse":"","book":""}},"5":{"spanish":{"verse":"Yo te guié de las tinieblas al reino de Mi amado Hijo.","book":"Colosenses 1:12-13"},"english":{"verse":"","book":""}},"6":{"spanish":{"verse":"Te he dado una vida eterna porque has creído en Jesucristo.","book":"Juan 3:16"},"english":{"verse":"","book":""}},"7":{"spanish":{"verse":"He enviado el Espíritu de Mi Hijo a tu corazón para que me puedas llamar Padre.","book":"Gálatas 4:6"},"english":{"verse":"","book":""}},"8":{"spanish":{"verse":"Desde que estás con Cristo te he hecho heredero de todas Mis promesas.","book":"Gálatas 3:29"},"english":{"verse":"","book":""}},"9":{"spanish":{"verse":"Yo te he dado un nuevo corazón y he puesto Mi propio Espíritu en ti.","book":"Ezequiel 36:26-27"},"english":{"verse":"","book":""}},"10":{"spanish":{"verse":"Mi plan para tu futuro está lleno de esperanza.","book":"Jeremías 29:11"},"english":{"verse":"","book":""}},"11":{"spanish":{"verse":"Yo obro en tu vida a través de los deseos de tu corazón.","book":"Filipenses 2:13"},"english":{"verse":"","book":""}},"12":{"spanish":{"verse":"Yo te daré pastos verdes para que descanses y te guiaré hacia aguas calmadas.","book":"Salmos 23:2"},"english":{"verse":"","book":""}},"13":{"spanish":{"verse":"Yo doy Mi Espíritu en medidas ilimitadas.","book":"Juan 3:34"},"english":{"verse":"","book":""}},"14":{"spanish":{"verse":"Yo voy a sostener tu mano para protegerte de cualquier caída.","book":"Salmos 37:24"},"english":{"verse":"","book":""}},"15":{"spanish":{"verse":"Yo te proveeré con todas las cosas buenas que necesites para cumplir Mi voluntad.","book":"Hebreos 13:20-21"},"english":{"verse":"","book":""}},"16":{"spanish":{"verse":"Yo te disciplinaré con amor como un padre que ama a sus hijos.","book":"Proverbios 3:11-12"},"english":{"verse":"","book":""}},"17":{"spanish":{"verse":"Mi poder descansará sobre ti cuando te sientas débil.","book":"2 Corintios 12:9"},"english":{"verse":"","book":""}},"18":{"spanish":{"verse":"Yo voy a bendecir tu vida y te cuidaré siempre.","book":"Números 6:24"},"english":{"verse":"","book":""}},"19":{"spanish":{"verse":"Si Me buscas con todo tu corazón, tú Me encontrarás.","book":"Jeremías 29:13"},"english":{"verse":"","book":""}},"20":{"spanish":{"verse":"Yo voy a proteger tu vida y te libraré de la malicia.","book":"Salmos 97:10"},"english":{"verse":"","book":""}},"21":{"spanish":{"verse":"Entrégame todas tu preocupaciones porque de verdad me importas.","book":"1 Pedro 5:7"},"english":{"verse":"","book":""}},"22":{"spanish":{"verse":"No dejaré que te tientan más allá de lo que tú puedas aguantar.","book":"1 Corintios 10:13"},"english":{"verse":"","book":""}},"23":{"spanish":{"verse":"Mi amor sobrevivirá en cualquier situación.","book":"1 Corintios 13:4-7"},"english":{"verse":"","book":""}},"24":{"spanish":{"verse":"Yo no retendré ninguna bendición de aquellos con un corazón puro.","book":"Salmos 84:11"},"english":{"verse":"","book":""}},"25":{"spanish":{"verse":"Yo busco los extranjeros y ayudo a los huérfanos y las viudas.","book":"Salmos 146:9"},"english":{"verse":"","book":""}},"26":{"spanish":{"verse":"Yo cuidaré aquellos que tengan un corazón puro.","book":"Salmos 116:6"},"english":{"verse":"","book":""}},"27":{"spanish":{"verse":"Tú eres más que vencedor por medio de Mi amor en Jesucristo.","book":"Romanos 8:37"},"english":{"verse":"","book":""}},"28":{"spanish":{"verse":"Cuando Jesús aparezca, recibirás una corona de gloria que nunca dejará de brillar.","book":"1 Pedro 5:3"},"english":{"verse":"","book":""}},"29":{"spanish":{"verse":"Al final del tiempo, Mis seguidores leales brillarán como el Sol en Mi reino.","book":"Mateo 13:43"},"english":{"verse":"","book":""}},"30":{"spanish":{"verse":"Ni una de Mis promesas te fallarán.","book":"Josué 23:14"},"english":{"verse":"","book":""}},"31":{"spanish":{"verse":"Estoy sentado en los cielos y Mi reino reina sobre todo.","book":"Salmos 103:13"},"english":{"verse":"","book":""}},"32":{"spanish":{"verse":"Todos mis planes se cumplirán porque Yo conozco el fin desde el principio.","book":"Isaías 46:10"},"english":{"verse":"","book":""}},"33":{"spanish":{"verse":"Te he conocido desde antes que nacieras y te he diseñado para mis propósitos.","book":"Jeremías 1:5"},"english":{"verse":"","book":""}},"34":{"spanish":{"verse":"Tú eres Santo y libre de culpa porque Jesucristo murió por ti.","book":"Colosenses 1:21-22"},"english":{"verse":"","book":""}},"35":{"spanish":{"verse":"Yo he borrado tus pecados y los he disuelto como la neblina.","book":"Isaías 44:22"},"english":{"verse":"","book":""}},"36":{"spanish":{"verse":"Yo te he sellado con Mi Espíritu para garantizar tu herencia.","book":"Efesios 1:13-14"},"english":{"verse":"","book":""}},"37":{"spanish":{"verse":"Yo voy a ser un verdadero Padre para ti.","book":"2 Corintios 6:18"},"english":{"verse":"","book":""}},"38":{"spanish":{"verse":"Yo te miraré y te mostraré el mejor camino.","book":"Salmos 32:8"},"english":{"verse":"","book":""}},"39":{"spanish":{"verse":"Confía en Mí con todo tu corazón y Yo te guiaré.","book":"Proverbios 3:5-6"},"english":{"verse":"","book":""}},"40":{"spanish":{"verse":"Yo te voy a dar paz en todos los momentos y en cualquier situación.","book":"2 Tesalonicenses 3:16"},"english":{"verse":"","book":""}},"41":{"spanish":{"verse":"Llámame cuando estés en problemas y Yo te salvaré.","book":"Salmos 91:15"},"english":{"verse":"","book":""}},"42":{"spanish":{"verse":"Yo soy tu amparo y el lugar seguro donde te puedes refugiar de tus enemigos.","book":"Salmos 61:3"},"english":{"verse":"","book":""}},"43":{"spanish":{"verse":"Si tú esperas por Mí, Yo trabajaré para tu bien.","book":"Isaías 64:4"},"english":{"verse":"","book":""}},"44":{"spanish":{"verse":"Mi amor nunca te fallará.","book":"1 Corintios 13:8"},"english":{"verse":"","book":""}},"45":{"spanish":{"verse":"Yo soy tu Pastor y voy a suplir todas tus necesidades.","book":"Salmos 23:1"},"english":{"verse":"","book":""}},"46":{"spanish":{"verse":"Yo te mantendré seguro porque nadie te puede tomar de Mi mano.","book":"Juan 10:29"},"english":{"verse":"","book":""}},"47":{"spanish":{"verse":"Yo voy a curar tu corazón y voy a atender todas tus heridas.","book":"Salmos 147:3"},"english":{"verse":"","book":""}},"48":{"spanish":{"verse":"Yo voy a bendecirte en tiempos de luto con Mi consuelo.","book":"Mateo 5:4"},"english":{"verse":"","book":""}},"49":{"spanish":{"verse":"Tú vas a encontrar libertad donde sea que Mi Espíritu habite.","book":"2 Corintios 3:17"},"english":{"verse":"","book":""}},"50":{"spanish":{"verse":"Tú puedes estar seguro y depender del amor que tengo por ti.","book":"1 Juan 4:16"},"english":{"verse":"","book":""}},"51":{"spanish":{"verse":"Tú puedes confiar en Mí porque nunca te olvidaré.","book":"Salmos 9:10"},"english":{"verse":"","book":""}},"52":{"spanish":{"verse":"Yo pongo a personas que están solitarias en familias.","book":"Salmos 68:6"},"english":{"verse":"","book":""}},"53":{"spanish":{"verse":"Yo te escucho cuando pides cualquier cosa conforme a Mi deseo.","book":"1 Juan 5:14"},"english":{"verse":"","book":""}},"54":{"spanish":{"verse":"Yo recompensaré aquellos que diligentemente Me busquen con un corazón de fe.","book":"Hebreos 11:6"},"english":{"verse":"","book":""}},"55":{"spanish":{"verse":"Yo te santificaré y te mantendré libre de culpa hasta que Jesús vuelva.","book":"1 Tesalonicenses 5:23-24"},"english":{"verse":"","book":""}},"56":{"spanish":{"verse":"Cuando tu cuerpo fallezca, tendrás una casa eterna esperando por ti.","book":"2 Corintios 5:1"},"english":{"verse":"","book":""}},"57":{"spanish":{"verse":"Aquellos que triunfen se sentarán con Mi Hijo en Su trono.","book":"Apocalipsis 3:21"},"english":{"verse":"","book":""}},"58":{"spanish":{"verse":"Mi promesa de vida es para ti y tu familia.","book":"Hechos 2:39"},"english":{"verse":"","book":""}},"59":{"spanish":{"verse":"El mundo me pertenece y todo lo que está en él.","book":"Salmos 24:1"},"english":{"verse":"","book":""}},"60":{"spanish":{"verse":"Mi trono estará de pie para siempre y la justicia reinará en Mi reino.","book":"Salmos 45:6"},"english":{"verse":"","book":""}},"61":{"spanish":{"verse":"El pasto se secará y las flores se marchitarán, pero Mi palabra durará para siempre.","book":"1 Pedro 1:24-25"},"english":{"verse":"","book":""}},"62":{"spanish":{"verse":"Aunque las montañas desaparezcan, Mi amor infinito nunca te dejará.","book":"Isaías 54:10"},"english":{"verse":"","book":""}},"63":{"spanish":{"verse":"Yo te he guiado de la oscuridad a Mi gloriosa luz.","book":"1 Pedro 2:9"},"english":{"verse":"","book":""}},"64":{"spanish":{"verse":"Yo he elegido las cosas débiles de este mundo para que avergüencen las cosas fuertes.","book":"1 Corintios 1:27"},"english":{"verse":"","book":""}},"65":{"spanish":{"verse":"Yo me contento en revelar Mi reino a aquellos con un corazón puro.","book":"Mateo 11:25-26"},"english":{"verse":"","book":""}},"66":{"spanish":{"verse":"Te acerqué a Mí a través de la sangre de Cristo.","book":"Efesios 2:13"},"english":{"verse":"","book":""}},"67":{"spanish":{"verse":"Tu vieja vida ha muerto y tu nueva vida está guardada junto con Cristo en Mí.","book":"Colosenses 3:3"},"english":{"verse":"","book":""}},"68":{"spanish":{"verse":"El mismo Espíritu que levantó a Jesús de los muertos también te dará vida a ti.","book":"Romanos 8:11"},"english":{"verse":"","book":""}},"69":{"spanish":{"verse":"Yo te he bendecido en Cristo con todas las bendiciones celestiales.","book":"Efesios 1:3"},"english":{"verse":"","book":""}},"70":{"spanish":{"verse":"Entrega todo lo que hagas a Mí y tus planes serán exitosos.","book":"Proverbios 16:3"},"english":{"verse":"","book":""}},"71":{"spanish":{"verse":"Yo estoy contigo y Yo siempre te ayudaré porque Yo soy tu Dios.","book":"Isaías 41:10"},"english":{"verse":"","book":""}},"72":{"spanish":{"verse":"Yo nunca te abandonaré.","book":"Hebreos 13:5"},"english":{"verse":"","book":""}},"73":{"spanish":{"verse":"Yo veo todos tus problemas y Me importan todos tus sufrimientos.","book":"Salmos 31:7"},"english":{"verse":"","book":""}},"74":{"spanish":{"verse":"Cuando pases por aguas turbulentas, Yo estaré contigo.","book":"Isaías 43:2"},"english":{"verse":"","book":""}},"75":{"spanish":{"verse":"Mi unción te enseñará todo lo que necesitas saber.","book":"1 Juan 2:27"},"english":{"verse":"","book":""}},"76":{"spanish":{"verse":"Si vives en Mi lugar secreto, encontrarás descanso bajo Mi sombra.","book":"Salmos 91:1"},"english":{"verse":"","book":""}},"77":{"spanish":{"verse":"Yo te voy a dar Mi fuerza para que puedas pararte en lugares altos.","book":"2 Samuel 22:33-34"},"english":{"verse":"","book":""}},"78":{"spanish":{"verse":"Miles podrán caer a tu alrededor pero nada te hará daño a ti.","book":"Salmos 91:7"},"english":{"verse":"","book":""}},"79":{"spanish":{"verse":"Yo le doy comida a los hambrientos y justicia a los oprimidos.","book":"Salmos 146:7"},"english":{"verse":"","book":""}},"80":{"spanish":{"verse":"Mi poder divino te dará todo lo que necesites para vivir una buena vida.","book":"2 Pedro 1:3"},"english":{"verse":"","book":""}},"81":{"spanish":{"verse":"Mi misericordia anulará cualquier sentencia.","book":"Santiago 2:13"},"english":{"verse":"","book":""}},"82":{"spanish":{"verse":"Yo haré que todas las cosas al final trabajen para tu bien.","book":"Romanos 8:28"},"english":{"verse":"","book":""}},"83":{"spanish":{"verse":"Humíllate ante Mí y te haré grande.","book":"1 Pedro 5:6"},"english":{"verse":"","book":""}},"84":{"spanish":{"verse":"Yo he tallado tu nombre en las palmas de Mis manos.","book":"Isaías 49:16"},"english":{"verse":"","book":""}},"85":{"spanish":{"verse":"Nunca te canses de hacer bien porque cosecharás en buena época.","book":"Gálatas 6:9"},"english":{"verse":"","book":""}},"86":{"spanish":{"verse":"Una corona eterna te esperará en la meta; así que sigue corriendo tu carrera.","book":"1 Corintios 9:24-25"},"english":{"verse":"","book":""}},"87":{"spanish":{"verse":"Yo he preparado como herencia un reino para ti cuando creé el mundo.","book":"Mateo 25:34"},"english":{"verse":"","book":""}},"88":{"spanish":{"verse":"Es precioso para Mí ver Mis fieles creyentes venir a casa.","book":"Salmos 116:15"},"english":{"verse":"","book":""}},"89":{"spanish":{"verse":"Yo nunca olvidaré Mis promesas porque soy un Dios misericordioso.","book":"Deuteronomio 4:31"},"english":{"verse":"","book":""}},"90":{"spanish":{"verse":"Mi reino durará para siempre y Mi dominio durará por siempre.","book":"Salmos 145:13"},"english":{"verse":"","book":""}},"91":{"spanish":{"verse":"Mi palabra eterna está anclada en los cielos.","book":"Salmos 119:89"},"english":{"verse":"","book":""}},"92":{"spanish":{"verse":"Yo soy un Dios clemente que es justo y está lleno de misericordia.","book":"Salmos 116:5"},"english":{"verse":"","book":""}},"93":{"spanish":{"verse":"Yo he creado todas las cosas para Mi placer.","book":"Apocalipsis 4:11"},"english":{"verse":"","book":""}},"94":{"spanish":{"verse":"Yo soy tu padre y tú eres Mi obra.","book":"Isaías 64:8"},"english":{"verse":"","book":""}},"95":{"spanish":{"verse":"Yo te protegeré y te miraré todos los días de tu vida.","book":"Isaías 46:4"},"english":{"verse":"","book":""}},"96":{"spanish":{"verse":"Yo he elegido que seas como Mi hijo para que puedas ser